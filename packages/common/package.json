{
    "private": true,
    "name": "common",
    "version": "1.0.1",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "scripts": {
        "lint": "prettier --cache-location=.prettiercache --check . && eslint . --report-unused-disable-directives --no-error-on-unmatched-pattern --max-warnings 0 && tsc",
        "lint:fix": "prettier --cache-location=.prettiercache --write . && eslint . --report-unused-disable-directives --no-error-on-unmatched-pattern --max-warnings 0 --fix && tsc"
    },
    "devDependencies": {
        "eslint": "*",
        "eslint-config-custom": "*",
        "prettier": "*",
        "prettier-config-custom": "*",
        "ts-config-custom": "*"
    }
}
