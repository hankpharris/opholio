{
    "buildCommand": "yarn build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "installCommand": "corepack enable && corepack prepare yarn@4.9.1 --activate && yarn install --immutable",
    "github": {
        "silent": true
    },
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    },
    "functions": {
        "app/api/**/*.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
} 
